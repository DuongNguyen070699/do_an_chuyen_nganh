{
  "compileOnSave": false,
  "compilerOptions": {
    "types": [
      "arcgis-js-api"
    ],
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "skipLibCheck": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "target": "es5",
    "lib": [
      "dom",
      "es2015.promise",
      "es5",
      "es6"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "paths": {
      "@shared":["src/app/shared"],
      "@shared/*":["src/app/shared/*"],
      "@environments/*":["src/environments/*"]
    }
  }
}
