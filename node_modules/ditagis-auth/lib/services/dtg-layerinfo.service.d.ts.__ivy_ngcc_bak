import { HttpClient } from '@angular/common/http';
import { DTGAuthModuleOptions } from '../dtg-auth.config';
import { Observable } from 'rxjs';
import { LayerEntity, LayerInfo } from '../interfaces';
import { DTGAuthService } from '../ditagis-auth.service';
export declare class DTGLayerInfoService {
    private http;
    private authService;
    private config;
    constructor(http: HttpClient, authService: DTGAuthService, config: DTGAuthModuleOptions);
    get(): Observable<LayerInfo[]>;
    getGuest(): Observable<LayerEntity[]>;
}
